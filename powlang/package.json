{
  "name": "powlang",
  "displayName": "PowLang",
  "description": "Syntax highlighting for PowLang",
  "version": "0.0.1",
  "publisher": "sopow",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "grammars": [
      {
        "language": "powlang",
        "scopeName": "source.powlang",
        "path": "./syntaxes/powlang.tmLanguage.json"
      }
    ],
    "languages": [
      {
        "id": "powlang",
        "aliases": ["PowLang", "powlang"],
        "extensions": [".pow"],
        "configuration": "./language-configuration.json"
      }
    ],
    "iconThemes": [
      {
        "id": "powlang-icons",
        "label": "PowLang Icons",
        "path": "./icons/powlang-icon-theme.json"
      }
    ]
  }
}
